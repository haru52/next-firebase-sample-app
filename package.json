{
  "name": "next-firebase-sample-app",
  "version": "0.1.0",
  "description": "Next.js on Firebase sample ToDo Web application.",
  "private": true,
  "engines": {
    "node": "^18.17.0",
    "npm": "^9.8.1"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "run-s --continue-on-error lint:*",
    "lint:credentials": "[ \"$CI\" = 'true' ] || secretlint \"**/*\"",
    "lint:md": "markdownlint . \".?*/**/*.{md,markdown}\"",
    "lint:prettier": "prettier --check .",
    "lint:next": "next lint",
    "format": "run-s --continue-on-error format:*",
    "format:md": "npm run lint:md -- --fix",
    "format:prettier": "prettier --write .",
    "format:es": "npm run lint:es -- --fix",
    "commit": "cz",
    "cm": "npm run commit",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@types/node": ">=18.0.0 <18.18.0",
    "@types/react": "^18.2.17",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.14",
    "bootstrap": "^5.3.1",
    "eslint": "^8.45.0",
    "eslint-config-next": "^13.4.12",
    "firebase": "^10.1.0",
    "next": "^13.4.12",
    "postcss": "^8.4.27",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swr": "^2.2.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@commitlint/cz-commitlint": "^17.6.7",
    "@secretlint/secretlint-rule-preset-recommend": "^7.0.3",
    "@types/bootstrap": "^5.2.6",
    "commitizen": "^4.3.0",
    "conventional-changelog-conventionalcommits": "^6.1.0",
    "firebase-tools": "^12.4.6",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "markdownlint-cli": ">=0.35.0 <1.0.0",
    "npm-check-updates": "^16.10.17",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "secretlint": "^7.0.3"
  }
}
